r\n        JOptionPane.showMessageDialog(null, this.name + \" slashes \" + enemy.getName() + \" with Phantom Claw for \"\r\n                + damage + \" damage!\");\r\n        JOptionPane.showMessageDialog(null, enemy.getName() + \" has \" + enemy.getStats().get(Stats.HP) + \" HP left.\");\r\n    }\r\n}\r\n
